{
	"_version": "1.81.0",
	"sap.app": {
		"id": "loanoffercreation",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			]
		},
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "@sap/generator-fiori:lrop",
			"version": "1.20.1",
			"toolsId": "0a27975c-226c-4337-a10b-265f46fc6178"
		},
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/my/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"loanofferapplication-Display": {
					"semanticObject": "loanofferapplication",
					"action": "Display",
					"title": "{{loanofferapplication-Display.flpTitle}}",
					"subTitle": "{{loanofferapplication-Display.flpSubtitle}}",
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					}
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"config": {
			"dateFormatSettings": {
				"pattern": "MM/dd/yyyy"
			}
		},
		"cacheBuster": {
			"enabled": true
		},
		"flexEnabled": false,
		"dependencies": {
			"minUI5Version": "1.143.2",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "loanoffercreation.i18n.i18n",
					"supportedLocales": [
						""
					]
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties",
				"settings": {
					"supportedLocales": [
						""
					]
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {},
			"routes": [
				{
					"pattern": ":?query:",
					"name": "ContractList",
					"target": "ContractList"
				},
				{
					"pattern": "Contract({key}):?query:",
					"name": "ContractObjectPage",
					"target": "ContractObjectPage"
				},
				{
					"name": "Contract_contractToEarmarkObjectPage",
					"pattern": "Contract({key})/contractToEarmark({contractToEarmarkKey}):?query:",
					"target": "Contract_contractToEarmarkObjectPage"
				},
				{
					"name": "Contract_contractToDisbursementObjectPage",
					"pattern": "Contract({key})/contractToDisbursement({contractToDisbursementKey}):?query:",
					"target": "Contract_contractToDisbursementObjectPage"
				}
			],
			"targets": {
				"ContractList": {
					"type": "Component",
					"id": "ContractList",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"contextPath": "/Contract",
							"variantManagement": "Page",
							"navigation": {
								"Contract": {
									"detail": {
										"route": "ContractObjectPage"
									}
								}
							},
							"controlConfiguration": {
								"@com.sap.vocabularies.UI.v1.LineItem": {
									"tableSettings": {
										"type": "GridTable"
									}
								}
							}
						}
					}
				},
				"ContractObjectPage": {
					"type": "Component",
					"id": "ContractObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"editableHeaderContent": false,
							"contextPath": "/Contract",
							"navigation": {
								"contractToEarmark": {
									"detail": {
										"route": "Contract_contractToEarmarkObjectPage"
									}
								},
								"contractToDisbursement": {
									"detail": {
										"route": "Contract_contractToDisbursementObjectPage"
									}
								}
							},
							"sectionLayout": "Tabs",
							"content": {
								"body": {
									"sections": {
										"Attachments": {
											"template": "loanoffercreation.ext.fragment.Attachments",
											"position": {
												"placement": "After",
												"anchor": "Disbursement"
											},
											"title": "Attachments"
										},
										"Workflowhistory": {
											"template": "loanoffercreation.ext.fragment.Workflowhistory",
											"position": {
												"placement": "After",
												"anchor": "Comments"
											},
											"title": "Workflow History"
										},
										"Changelogs": {
											"template": "loanoffercreation.ext.fragment.Changelogs",
											"position": {
												"placement": "After",
												"anchor": "Workflowhistory"
											},
											"title": "Change Logs"
										},
										"Comments": {
											"template": "loanoffercreation.ext.fragment.Comments",
											"position": {
												"placement": "After",
												"anchor": "Attachments"
											},
											"title": "Comments"
										},
										"Partners": {
											"subSections": {
												"Incomingpayment": {
													"template": "loanoffercreation.ext.fragment.Incomingpayment",
													"position": {
														"placement": "After",
														"anchor": "Partners1"
													},
													"title": "Incoming Payment"
												}
											}
										}
									}
								},
								"header": {
									"facets": {
										"Header": {
											"template": "loanoffercreation.ext.fragment.Header",
											"title": "Header Details",
											"stashed": false,
											"requestGroupId": "Heroes",
											"flexSettings": {
												"designtime": "Default"
											}
										}
									}
								}
							},
							"controlConfiguration": {
								"contractToCondition/@com.sap.vocabularies.UI.v1.LineItem#ConditionItems": {
									"actions": {
										"rate": {
											"press": "loanoffercreation.ext.controller.Rate.Rate",
											"visible": true,
											"enabled": true,
											"requiresSelection": false,
											"text": "Rate"
										}
									}
								},
								"@com.sap.vocabularies.UI.v1.FieldGroup#_": {
									"actions": {
										"calculatecashflow": {
											"press": "loanoffercreation.ext.controller.CalculateCashflow.CalculateCashflow",
											"visible": true,
											"enabled": true,
											"text": "Calculate CashFlow"
										}
									}
								},
								"contractToPartner/@com.sap.vocabularies.UI.v1.LineItem#Partners": {
									"columns": {
										"DataField::title": {
											"width": "14%"
										},
										"DataField::nameAddress": {
											"width": "14%"
										},
										"DataField::startRel": {
											"width": "14%"
										},
										"DataField::endRel": {
											"width": "14%"
										},
										"DataField::partner": {
											"width": "14%"
										},
										"DataField::bpRole": {
											"width": "14%"
										},
										"Selection": {
											"header": "Action",
											"width": "16%",
											"position": {
												"anchor": "DataField::endRel",
												"placement": "After"
											},
											"template": "loanoffercreation.ext.fragment.Selection"
										}
									}
								},
								"contractToEarmark/@com.sap.vocabularies.UI.v1.LineItem#_": {
									"columns": {
										"DataField::documentNumber": {
											"width": "12%"
										},
										"DataField::loanCategory": {
											"width": "9%"
										},
										"DataField::isCompleted": {
											"width": "12%"
										},
										"DataField::orginalAount": {
											"width": "12%"
										},
										"DataField::text": {
											"width": "12%"
										},
										"DataField::fund": {
											"width": "12%"
										},
										"DataField::budgetPd": {
											"width": "12%"
										},
										"DataField::glAccount": {
											"width": "12%"
										},
										"DataField::costCenter": {
											"width": "9%"
										},
										"DataField::order": {
											"width": "9%"
										},
										"DataField::wbsElement": {
											"width": "9%"
										}
									}
								},
								"contractToDisbursement/@com.sap.vocabularies.UI.v1.LineItem#Disbursements": {
									"columns": {
										"DataField::bp": {
											"width": "20%"
										},
										"DataField::text": {
											"width": "20%"
										},
										"DataField::paymentDate": {
											"width": "20%"
										},
										"DataField::disberseAmount": {
											"width": "20%"
										},
										"DataField::effectiveDate": {
											"width": "20%"
										}
									}
								}
							}
						}
					}
				},
				"Contract_contractToEarmarkObjectPage": {
					"type": "Component",
					"id": "Contract_contractToEarmarkObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"navigation": {},
							"contextPath": "/Contract/contractToEarmark"
						}
					}
				},
				"Contract_contractToDisbursementObjectPage": {
					"type": "Component",
					"id": "Contract_contractToDisbursementObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"navigation": {},
							"contextPath": "/Contract/contractToDisbursement"
						}
					}
				}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.controllerExtensions": {
					"sap.fe.templates.ListReport.ListReportController": {
						"controllerName": "loanoffercreation.ext.controller.Listcontr"
					},
					"sap.fe.templates.ObjectPage.ObjectPageController": {
						"controllerName": "loanoffercreation.ext.controller.Contractobj"
					}
				}
			}
		},
		"flexBundle": false
	},
	"sap.fiori": {
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.fe": {
		"app": {
			"enableLazyLoading": true
		}
	},
	"sap.cloud": {
		"public": true,
		"service": "management"
	}
}